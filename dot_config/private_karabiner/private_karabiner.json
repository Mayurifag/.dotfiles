{
    "global": {
        "check_for_updates_on_startup": false,
        "show_in_menu_bar": false
    },
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "Swap Option and Command keys on external keyboards (Windows keyboard onto Mac layout)",
                        "manipulators": [
                            {
                            "type": "basic",
                            "from": {
                                "key_code": "left_option",
                                "modifiers": { "optional": ["any"] }
                            },
                            "to": [{ "key_code": "left_command" }],
                            "conditions": [
                                {
                                "type": "device_unless",
                                "identifiers": [
                                    {
                                    "is_built_in_keyboard": true
                                    }
                                ]
                                }
                            ]
                            },
                            {
                            "type": "basic",
                            "from": {
                                "key_code": "left_command",
                                "modifiers": { "optional": ["any"] }
                            },
                            "to": [{ "key_code": "left_option" }],
                            "conditions": [
                                {
                                "type": "device_unless",
                                "identifiers": [
                                    {
                                    "is_built_in_keyboard": true
                                    }
                                ]
                                }
                            ]
                            },
                            {
                            "type": "basic",
                            "from": {
                                "key_code": "right_option",
                                "modifiers": { "optional": ["any"] }
                            },
                            "to": [{ "key_code": "right_command" }],
                            "conditions": [
                                {
                                "type": "device_unless",
                                "identifiers": [
                                    {
                                    "is_built_in_keyboard": true
                                    }
                                ]
                                }
                            ]
                            },
                            {
                            "type": "basic",
                            "from": {
                                "key_code": "right_command",
                                "modifiers": { "optional": ["any"] }
                            },
                            "to": [{ "key_code": "right_option" }],
                            "conditions": [
                                {
                                "type": "device_unless",
                                "identifiers": [
                                    {
                                    "is_built_in_keyboard": true
                                    }
                                ]
                                }
                            ]
                            }
                        ]
                    },
                    {
                        "description": "Fn + Backspace -> Forward Delete (macbook doesnt have Delete key)",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "delete_or_backspace",
                                    "modifiers": { "mandatory": ["fn"] }
                                },
                                "to": [{ "key_code": "delete_forward" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Command + Esc -> Command + ` (prevents missclick for terminal opening)",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com.raycast.macos"
                                        ],
                                        "type": "frontmost_application_unless"
                                    }
                                ],
                                "from": {
                                    "key_code": "escape",
                                    "modifiers": { "mandatory": ["left_command"] }
                                },
                                "to": [
                                    {
                                        "key_code": "grave_accent_and_tilde",
                                        "modifiers": ["left_command"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Dota 2: Disable Cmd+W, Cmd+Q, Cmd+E, Option+E",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com.valvesoftware.dota2"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": {
                                    "simultaneous": [
                                        {
                                            "key_code": "q",
                                            "modifiers": { "mandatory": ["command"] }
                                        },
                                        {
                                            "key_code": "w",
                                            "modifiers": { "mandatory": ["command"] }
                                        },
                                        {
                                            "key_code": "e",
                                            "modifiers": { "mandatory": ["command"] }
                                        },
                                        {
                                            "key_code": "e",
                                            "modifiers": { "mandatory": ["option"] }
                                        }
                                    ]
                                },
                                "to": [{ "key_code": "vk_none" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Command+E Opens Finder (except for remote desktop apps) like on Windows",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.microsoft\\.rdc$",
                                            "^com\\.microsoft\\.rdc\\.mac$",
                                            "^com\\.microsoft\\.rdc\\.macos$",
                                            "^com\\.microsoft\\.rdc\\.osx\\.beta$",
                                            "^net\\.sf\\.cord$",
                                            "^com\\.thinomenon\\.RemoteDesktopConnection$",
                                            "^com\\.itap-mobile\\.qmote$",
                                            "^com\\.nulana\\.remotixmac$",
                                            "^com\\.p5sys\\.jump\\.mac\\.viewer$",
                                            "^com\\.p5sys\\.jump\\.mac\\.viewer\\.web$",
                                            "^com\\.teamviewer\\.TeamViewer$",
                                            "^com\\.vmware\\.horizon$",
                                            "^com\\.2X\\.Client\\.Mac$",
                                            "^com\\.vmware\\.fusion$",
                                            "^com\\.vmware\\.horizon$",
                                            "^com\\.vmware\\.view$",
                                            "^com\\.parallels\\.desktop$",
                                            "^com\\.parallels\\.vm$",
                                            "^com\\.parallels\\.desktop\\.console$",
                                            "^org\\.virtualbox\\.app\\.VirtualBoxVM$",
                                            "^com\\.citrix\\.XenAppViewer$",
                                            "^com\\.vmware\\.proxyApp\\.",
                                            "^com\\.parallels\\.winapp\\.",
                                            "^com.raycast.macos",
                                            "^com.valvesoftware.dota2",
                                            "^com.larian.bg3"
                                        ],
                                        "type": "frontmost_application_unless"
                                    }
                                ],
                                "from": {
                                    "key_code": "e",
                                    "modifiers": { "mandatory": ["command"] }
                                },
                                "to": [{ "shell_command": "open -a 'Finder.app'" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "CapsLock to Command+SpaceBar (changes language). Except Dota 2",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com.valvesoftware.dota2"
                                        ],
                                        "type": "frontmost_application_unless"
                                    }
                                ],
                                "from": { "key_code": "caps_lock" },
                                "to": [
                                    {
                                        "key_code": "spacebar",
                                        "modifiers": ["left_command"],
                                        "repeat": false
                                    }
                                ],
                                "to_if_held_down": [
                                    {
                                        "key_code": "spacebar",
                                        "modifiers": ["left_command"],
                                        "repeat": true
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Dota 2: CapsLock to 'l' (because of double-click on the CapsLock bug)",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com.valvesoftware.dota2"
                                        ],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": { "key_code": "caps_lock" },
                                "to": [
                                    {
                                        "key_code": "l",
                                        "repeat": false
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Mac OSX: disable cmd + h to prevent minimising an application window",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "h",
                                    "modifiers": { "mandatory": ["command"] }
                                },
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Mac OSX: disable cmd + option + h + m to prevent minimising all windows",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "h",
                                    "modifiers": {
                                        "mandatory": ["command", "option"],
                                        "optional": ["any"]
                                    }
                                },
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "m",
                                    "modifiers": {
                                        "mandatory": ["command", "option"],
                                        "optional": ["any"]
                                    }
                                },
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Mouse5 to Fn - for using wispr flow conveniently",
                        "manipulators": [
                            {
                                "from": { "pointing_button": "button5" },
                                "to": [{ "key_code": "fn" }],
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
            "devices": [
                {
                    "identifiers": {
                        "is_pointing_device": true,
                        "product_id": 62735,
                        "vendor_id": 13652
                    },
                    "ignore": false
                }
            ],
            "name": "Default profile",
            "selected": true
        }
    ]
}